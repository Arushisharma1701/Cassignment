#include <stdio.h>#defineBASE10int main(){
long long num, n;inti, lastDigit;
intfreq[BASE];
printf("Enteranynumber:");scanf("%lld",&num);
for(i=0;i<BASE;i++){freq[i]=0;}
n = num;while(n!=0{
lastDigit=n%10;n /= 10;freq[lastDigit]++}
 
printf("Frequency of each digit in %lld is: \n", num);for(i=0;i<BASE;i++){
printf("Frequencyof%d=%d\n",i,freq[i]);}return 0;}





