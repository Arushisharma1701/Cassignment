#include<stdio.h>intmain(){
int side1, side2, side3;printf("Entersidesoftriangle:");
scanf("%d%d%d",&side1,&side2,&side3);if(side1==side2&&side2==side3)
printf("TheGivenTriangleisequilateral");
else if(side1 == side2 || side2 == side3 || side3 == side1)printf("ThegivenTriangleisisosceles");
else
printf("The given Triangle is scalene");return0;
}


