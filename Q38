#include <stdio.h>#defineSIZE 3
intmain(){
intA[SIZE][SIZE];
intnum, row,col;
printf("Enter elements in matrix of size %dx%d: \n", SIZE,SIZE);
for(row=0;row<SIZE;row++)
{

for(col=0;col<SIZE;col++)
{

scanf("%d",&A[row][col]);
}
}

printf("Enter any number to multiply with matrix A: ");scanf("%d",&num);
for(row=0;row<SIZE;row++){
 
for(col=0; col<SIZE; col++){A[row][col]=num*A[row][col];
}
}

printf("\n Resultant matrix c.A = \n");for(row=0;row<SIZE;row++) {
for(col=0; col<SIZE; col++){printf("%d",A[row][col]);}
printf("\n");

}
return0;
}



